


#include<bits/stdc++.h>
using namespace std;



int main(){
    vector<int>arr ={1,2,4,3,3,3,3,5} ;
    map<int,int>mapp;
    
    
    for (int i = 0; i < arr.size(); i++) {
        
            mapp[arr[i]]++;
       
      
    }
    for (auto it : mapp) {
        if (it.second > 1) {  
            cout << it.first << " duplicate "<< it.second << " times "<< endl;
        }
    }
   
    
}