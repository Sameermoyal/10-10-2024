


#include<bits/stdc++.h>
using namespace std;



int main(){
    vector<int>arr ={1,2,4,16,32} ;
    int missingNumber = -1;
    int ratio = arr[1] / arr[0];  
    
    for (int i = 1; i < arr.size(); i++) {
      
        if (arr[i] != arr[i - 1] * ratio) {
            missingNumber = arr[i - 1] * ratio;
            break;  
        }
    }
    
    cout << "The missing number is: " << missingNumber << endl;
    
}